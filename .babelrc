{
  "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
          "corejs": 3,
          "regenerator": true,
          "useESModules": false,
          "absoluteRuntime": true
      }
    ],
    [
      "react-hot-loader/babel"
    ],
    ["@babel/plugin-proposal-class-properties"]
  ]
}